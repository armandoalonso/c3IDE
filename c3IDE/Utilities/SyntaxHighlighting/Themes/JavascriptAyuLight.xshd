  <Color name="Background" foreground="#FAFAFA" />
  <Color name="Foreground" foreground="#959DA6" />

  <Color name="Interfaces" foreground="#55B4D4" />
  <Color name="Functions" foreground="#F2AE49" />
  <Color name="Strings" foreground="#86B300" />
  <Color name="Character" foreground="#ED9366" />
